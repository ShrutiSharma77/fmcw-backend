<%- include('partials/header.ejs') %>
<%- include('admintop.ejs') %>															
	<hr>
	<main>
<div class="table-data">
<%  Details.forEach((data) => { %>	
	<div class="wrapper">
		<div class="left">
			<br><br>
			<img src="https://th.bing.com/th/id/OIP.Pzit_z06f4SRCUcACGIsAwHaHa?pid=ImgDet&rs=1" alt="user" width="100">
			<br><br>		<h3><%=data.name%></h3>
			<hr>			
			
			
		</div>
		<div class="right">
			<div class="info">
				<h3>Information</h3>
				<div class="info_data">
					
					
					<div class="data">
						<h4>College</h4>
						<p><%=data.college%></p>
					</div>
					<div class="data">
						<h4>Transaction ID</h4>
						<p><%=data.transactionID%></p>
					</div>
					<div class="data">
						<h4>Insta Handle</h4>
						<p><%=data.instaHandle%></p>
					</div>
					<div class="data">
						<h4>Current Year</h4>
						<p><%=data.yearOfStudy%></p>
					</div>
					<div class="data">
						<h4>Phone</h4>
						<p><%=data.number%></p>
					</div>
					<div class="data">
						<h4>Email</h4>
						<p> <%=data.email%></p>
					</div>
				</div>
			</div>
			
			<div class="projects">
				<h3>Orders</h3>
				
				<div class="projects_data">
					
					<div class="data">
						<h4>Title</h4>
						<%  data.cartItems.forEach((datas) => { %>
							<p><%=datas.title%></p>
							<% }); %>
						</div>
						<div class="data">
							<h4>Price</h4>
							<%  data.cartItems.forEach((datas) => { %>
								<p> <%=datas.price%></p>
								<% }); %>
							</div>
							<div class="data">
								<h4>Payment Status</h4>
								<%  data.cartItems.forEach((datas) => { %>
									<p><%=datas.verifyStatus%></p>
									<% }); %>
								</div>
								<div class="data">
									<h4>Genre</h4>
									<%  data.cartItems.forEach((datas) => { %>
										<p>	<%=datas.genre%></p>
										<% }); %>
									</div>
									<div class="data">
										<h4>Prize</h4>
										<%  data.cartItems.forEach((datas) => { %>
											<p>	<%=datas.prize%></p>
											<% }); %>
										</div>
										
									</div>
								</div>
								
								<div class="social_media">
									
								</div>
							</div>
						</div>	<% }); %>
						
					</div>			
				
																		<%- include('partials/footer.ejs') %>																		